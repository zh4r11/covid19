	<queryString>
		<![CDATA[SELECT
     rukuntetangga.`nomor_rt` AS rukuntetangga_nomor_rt,
     kepalakeluarga.`nama` AS kepalakeluarga_nama,
     kepalakeluarga.`alamat` AS kepalakeluarga_alamat,
     kepalakeluarga.`jumlahanggota` AS kepalakeluarga_jumlahanggota,
     penerima.`id_penerima` AS penerima_id_penerima,
     penerima.`id_kk` AS penerima_id_kk
FROM
     `kepalakeluarga` kepalakeluarga INNER JOIN `penerima` penerima ON kepalakeluarga.`id_kk` = penerima.`id_kk`
     INNER JOIN `rukuntetangga` rukuntetangga ON kepalakeluarga.`id_rt` = rukuntetangga.`nomor_rt`
where penerima.`keterangan` = "Tidak Berhak"
ORDER BY rukuntetangga.`nomor_rt`]]>
	</queryString>
